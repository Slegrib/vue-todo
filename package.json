{
  "name": "vue-todo",
  "version": "0.0.1",
  "private": true,
  "author": "Scott Birgel",
  "engines": {
    "node": "6"
  },
  "scripts": {
    "start": "NODE_ENV=development babel-node server.js",
    "dev:server": "NODE_ENV=development babel-node server.js",
    "hot": "NODE_ENV=development babel-node ./webpackConfig/webpack.server.babel.js",
    "prod:build:client": "npm run clobber:client && NODE_ENV=production  webpack --config ./webpackConfig/webpack.client.prod.babel.js",
    "prod:build:server": "npm run clobber:server && NODE_ENV=production  webpack --config ./webpackConfig/webpack.server.prod.babel.js",
    "prod:build:both": "npm run prod:build:client && npm run prod:build:server",
    "clobber:client": "rm -rf public/dist",
    "clobber:server": "rm -rf dist",
    "prod": "npm run prod:build:both && NODE_ENV=production babel-node server.js"
  },
  "dependencies": {
    "autosize": "^3.0.20",
    "axios": "^0.15.3",
    "babel-eslint": "^7.2.1",
    "body-parser": "^1.17.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.18.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "express": "^4.14.0",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "vue": "^2.1.10",
    "vue-meta": "^0.5.3",
    "vue-router": "^2.1.2",
    "vue-server-renderer": "^2.1.10",
    "vuex": "^2.2.1",
    "vuex-router-sync": "^4.1.2",
    "webpack": "^1.14.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "memory-fs": "^0.4.1",
    "node-sass": "^4.3.0",
    "sass-loader": "^4.1.1",
    "vue-loader": "^10.0.2",
    "vue-template-compiler": "^2.1.10",
    "webpack-dev-server": "^1.16.2",
    "webpack-manifest-plugin": "^1.1.0",
    "webpack-md5-hash": "0.0.5"
  }
}
